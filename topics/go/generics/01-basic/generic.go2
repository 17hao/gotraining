package main

import "fmt"

func main() {
	numbers := []int{1, 2, 3}
	print(numbers)

	strings := []string{"A", "B", "C"}
	print(strings)
}

// =============================================================================

func print(type T)(list []T) {
	for _, v := range list {
		fmt.Print(v, " ")
	}
	fmt.Print("\n")
}
